{
  "name": "daily-planner-main",
  "version": "1.0.0",
  "description": "MongoDB Express Node Typescript REST API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "start-dev": "npx nodemon",
    "test": "jest --config jest.config.ts",
    "testdb": "cross-env NODE_ENV=test npx ts-node ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielaLukacova/daily-planner.git"
  },
  "author": "Gabriela and Emma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GabrielaLukacova/daily-planner/issues"
  },
  "homepage": "https://github.com/GabrielaLukacova/daily-planner#readme",
  "nodemonConfig": {
    "verbose": false,
    "watch": [
      "src"
    ],
    "ext": "js,ts",
    "exec": "npx ts-node ./src/index.ts",
    "ignore": [
      "node_modules"
    ]
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^4.1.0",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "mongoose": "^8.12.1",
    "node-cron": "^4.0.5",
    "nodemon": "^3.1.9",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
